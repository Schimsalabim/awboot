+			"ADCL", "Left ADC",
+			"ADCR", "Right ADC",
+			"MIC2", "Microphone Jack";
+
+	status = "disabled";
+};
+*/
+
+&npu {
+	status = "okay";
+};
+
+&dmic {
+	status = "okay";
+};
